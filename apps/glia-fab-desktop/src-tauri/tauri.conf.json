{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Glia Fab Desktop",
    "version": "0.1.0"
  },
  "tauri": {
    "windows": [
      {
        "title": "Glia Fab Desktop",
        "width": 1400,
        "height": 900,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "identifier": "com.gliafab.desktop",
      "icon": []
    }
  }
}
